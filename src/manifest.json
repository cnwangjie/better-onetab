{
	"manifest_version" : 2,
	"name" : "扩展管理器",
	"version" : "3.3",
	"description" : "一键管理Chrome浏览器的扩展，操作简单方便，减少扩展对资源的占用及浪费，绿色安全。",
	"browser_action" : {
		"default_icon" : "icon/browser_action_icon.png",
		"default_title": "扩展管理器",
		"default_popup": "popup.html"
	},
	"permissions" : ["management"],
	"options_page" : "options.html",
	"icons": {
		"16" : "icon/icon_16.png",
		"48" : "icon/icon_48.png",
		"128" : "icon/icon_128.png"
	}
}
